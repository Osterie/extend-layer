    "Monitor":{
        "ObjectName": "MonitorInstance",
        "ClassName": "Monitor",
        "Description": "Used for controlling the monitor, can change brightness and gamma",
        "Methods": {
            "SetBrightness": {
                "MethodName": "SetBrightness",
                "Description": "Sets the brightness of the monitor",
                "Parameters": [
                    {
                        "Name": "Brightness",
                        "Type": "int",
                        "Description": "The brightness to set the monitor to. 0-100"
                    }
                ]
            },
            "ToggleHighestBrightness": {
                "MethodName": "ToggleHighestBrightness",
                "Description": "Toggles the brightness of the monitor to 100 and 50. Might only work for laptops, and not yet tested on other devices(than my own).",
                "Parameters": []
            },
            "ToggleLowestBrightness":{
                "MethodName": "ToggleLowestBrightness",
                "Description": "Toggles the brightness of the monitor to 0 and 50. Might only work for laptops, and not yet tested on other devices(than my own).",
                "Parameters": []
            },

            "SetGamma": {
                "MethodName": "SetGamma",
                "Description": "Sets the gamma of the monitor",
                "Parameters": [
                    {
                        "Name": "Red",
                        "Type": "int",
                        "Description": "The red value to set the gamme. 0-255"
                    },
                    {
                        "Name": "Green",
                        "Type": "int",
                        "Description": "The green value to set the gamme. 0-255"
                    },
                    {
                        "Name": "Blue",
                        "Type": "int",
                        "Description": "The blue value to set the gamme. 0-255"
                    }
                ]

            },
            "ToggleHighestGamma": {
                "MethodName": "ToggleHighestGamma",
                "Description": "Toggles the gamma of the monitor to (255, 255, 255) and (128, 128, 128). Might only work for laptops, and not yet tested on other devices(than my own).",
                "Parameters": []
            },
            "ToggleLowestGamma":{
                "MethodName": "ToggleLowestGamma",
                "Description": "Toggles the gamma of the monitor to (0, 0, 0) and (128, 128, 128). Might only work for laptops, and not yet tested on other devices(than my own).",
                "Parameters": []
            }
        }
    },
